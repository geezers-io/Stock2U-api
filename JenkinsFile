def CIStart() {
    def workspace = env.JOB_URL.substring(41, 59)
    def branch = env.GIT_BRANCH
    def buildUrl = env.BUILD_URL

    def message = """
*[${workspace}]*
빌드 번호: ${env.BUILD_NUMBER}
<${buildUrl}|${branch}> 에 대한 빌드를 수행할게요!
잠시만 기다려주시고 작업하시느라 고생하셨습니다 :kirby_dance:    
    """

    def slack = slackSend (
            channel: '#해커톤-jenkins',
            color: '#6499E9',
            message: message
    )
    slack.addReaction('typingcat')
}

pipeline {
    agent any

    stages {
        stage('start') {
            steps {
                sh 'printenv'
                CIStart()
            }
        }
    }
}

