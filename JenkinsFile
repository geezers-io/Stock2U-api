def CIStart(name) {
    def message = """
        아아, 슬랙 테스트, 자그마치 3시간이나 걸렸다고 한다.
    """
    slackSend (
            channel: '#해커톤-jenkins',
            color: '#6499E9',
            message: message
    )
}

pipeline {
    agent any

    stages {
        stage('start') {
            steps {
                sh 'printenv'
                CIStart('')
            }
        }
    }
}

