def CIStart(name) {
    def message = """
        이모티콘 테스트 귀엽게
    """
    return slackSend (
            channel: '#해커톤-jenkins',
            color: '#6499E9',
            message: message
    )
}

pipeline {
    agent any

    stages {
        stage('start') {
            steps {
                sh 'printenv'
                def slack = CIStart('')
                slack.addReaction('typingcat')
            }
        }
    }
}

